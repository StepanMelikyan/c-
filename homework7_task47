//task47

double[,] array = new double[5, 5];
Random random = new Random();
for (int i = 0; i < array.GetLength(0); i++)
{
    for (int j = 0; j < array.GetLength(1); j++)
    {
        array[i, j] = random.NextDouble() * 10;
        Console.Write("{0,6:F2}", array[i, j]);
    }
    Console.WriteLine();
}

